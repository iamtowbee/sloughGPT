#!/usr/bin/env python3
"""
SLO - Interactive CLI Entry Point

This is the main entry point for the SLO interactive CLI system.
Usage:
    slo                    # Interactive mode
    slo train small       # Command mode
    slo config list       # Command mode
    slo help              # Show help
"""

import sys
from pathlib import Path

# Add project root to path
ROOT_DIR = Path(__file__).resolve().parent
sys.path.insert(0, str(ROOT_DIR))

from slo_cli import main

if __name__ == "__main__":
    main()