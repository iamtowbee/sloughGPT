# SloughGPT Production Configuration
SLOGH_GPT_WEBUI_NAME=SloughGPT
WEBUI_NAME=SloughGPT
WEBUI_FAVICON_URL=https://sloughgpt.com/favicon.png

# Server Configuration
HOST=0.0.0.0
PORT=8080
CORS_ALLOW_ORIGIN=*
